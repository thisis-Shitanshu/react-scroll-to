{
  "name": "react-scroll-to",
  "version": "2.0.5",
  "description": "Scroll to a position using react",
  "main": "./dist/react-scroll-to.js",
  "types": "./dist/definitions",
  "scripts": {
    "test": "jest",
    "test:cover": "jest --coverage",
    "build": "webpack --config=./config/webpack.config.js --mode=development",
    "build:prod": "webpack --config=./config/webpack.config.js --mode=production",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build:docs": "build-storybook -c .storybook -o docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ganderzz/react-scroll-to.git"
  },
  "keywords": [
    "react",
    "scroll",
    "to",
    "window",
    "position"
  ],
  "author": "Dylan Paulus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ganderzz/react-scroll-to/issues"
  },
  "homepage": "https://github.com/ganderzz/react-scroll-to#readme",
  "peerDepedencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@storybook/react": "4.1.11",
    "@types/jest": "^24.0.0",
    "@types/node": "^10.12.24",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "all-contributors-cli": "6.1.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "babel-loader": "8.0.5",
    "copy-webpack-plugin": "4.6.0",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.9.1",
    "eslint": "5.13.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "husky": "1.3.1",
    "jest": "24.1.0",
    "lint-staged": "8.1.3",
    "prettier": "1.16.4",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "react-testing-library": "5.5.4",
    "ts-loader": "5.3.3",
    "typescript": "^3.3.3",
    "webpack": "4.29.3",
    "webpack-cli": "3.2.3"
  },
  "jest": {
    "setupFiles": [
      "./src/tests/setup/shim.js",
      "./src/tests/setup/setupTests.js"
    ],
    "collectCoverageFrom": [
      "src/*.{js,jsx}",
      "!src/definitions",
      "!src/tests",
      "!src/{setupTests,shim}.js",
      "!src/index.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "create-react-context": "^0.2.3"
  }
}
